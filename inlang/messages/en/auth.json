{
  "$schema": "https://inlang.com/schema/inlang-message-format",
  "auth_email_error": [{
    "match": {
      "type=invalid": "The email provided is not valid",
      "type=empty": "You must provide an email"
    }
  }],
  "auth_email_label": "Email address",
  "auth_email_placeholder": "you@example.com",
  "auth_error_title": "Error",
  "auth_password_error": [{
    "match": {
      "type=invalid": "The password provided is not valid",
      "type=empty": "You must provide a password",
      "type=min_length": "Your password is not secure enough",
      "type=secure": "Your password is not secure enough"
    }
  }],
  "auth_password_label": "Password",
  "auth_password_placeholder": "8 characters minimum",
  "auth_sign_in_footer": "Don't have an account?",
  "auth_sign_in_forgot_password": "Forgot your password?",
  "auth_sign_in_go_to_sign_up": "Don't have an account?",
  "auth_sign_in_subtitle": "Sign in to your account",
  "auth_sign_in_title": "Welcome back",
  "auth_sign_up_error_first_name": [{
    "match": {
      "type=invalid": "The first name provided is not valid",
      "type=empty": "You must provide a first name"
    }
  }],
  "auth_sign_up_error_last_name": [{
    "match": {
      "type=invalid": "The last name provided is not valid",
      "type=empty": "You must provide a last name"
    }
  }],
  "auth_sign_up_first_name_label": "First name",
  "auth_sign_up_first_name_placeholder": "Jane",
  "auth_sign_up_footer": "Already with an account?",
  "auth_sign_up_go_to_sign_in": "Already with an account?",
  "auth_sign_up_last_name_label": "Last name",
  "auth_sign_up_last_name_placeholder": "Doe",
  "auth_sign_up_subtitle": "Join now and start controlling your gear!",
  "auth_sign_up_title": "First time here?"
}

